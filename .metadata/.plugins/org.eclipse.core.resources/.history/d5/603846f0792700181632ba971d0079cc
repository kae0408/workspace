
public class Person {
	// 陰蔽部 privateだと他のクラスからアクセスできない　public はアクセスできる
	private String name = null;
	private int age = 0;
	
	public Person(String name, int age) {
		this.name = name;
		this.age = age;
	}
	
	
	public String getName() {
		return this.name;
	}
	public void setName(String name) {
		this.name =name;
	}

}
