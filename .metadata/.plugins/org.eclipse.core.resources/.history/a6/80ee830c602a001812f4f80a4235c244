package rensyu;

public class main {

	public static void main(String[] args) {
		// TODO 自動生成されたメソッド・スタブ
		int a[] = {210,19,72,129,34};
		int b = a.length;
		int i, j, k;
		
		System.out.println("データの表示");
		for(i =0; i<b; i++);
			System.out.println(a[i] + "");
		System.out.println();
		
		for(j=0; j < b-1; j++) {
			for(i = j+i; i<b; i++) {
				if(a[j] > a[i]) {
					k = a[j];
					a[j] = a[i];
					a[j] = a[i];
					a[i] = k;
				}
			}
		}
		sysout
		}
	}

}
