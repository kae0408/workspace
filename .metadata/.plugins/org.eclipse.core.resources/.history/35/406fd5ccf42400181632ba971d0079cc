
public class Person2 {
	String name;
	int birthday;
	int manpukudo;
	
	Person2(String name,int birthday){
		this.name = name;
		this.birthday =birthday;
		this.manpukudo = 50;
	}
	
	Person2(){
		this.name = "不明";
		this.birthday =0;
		this.manpukudo = 50;
	}
	
	
	
	void eat() {
		this.manpukudo += 60;
	}
}
