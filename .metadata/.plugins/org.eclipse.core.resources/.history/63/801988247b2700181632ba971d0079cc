
public class SmartPhone extends Android implements Mp3Player,NewFunction {
	public void play() {
		System.out.println("再生する");
	 }
	public void next() {
		System.out.println("次へ");
	 }
	
	public void back() {
		System.out.println("戻る");
	 }
	
	public void call() {
		System.out.println("電話する");
	}
	 
	 public void mail() {
		System.out.println("メールする");
	 }
	 
	public void photo() {
		System.out.println("写真たくさんとるよ");
	}

	
}
