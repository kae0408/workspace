
public class SmartPhone  ectends Phone implements Mp3Player, NewFunction {
	public void play() {
		System.out.println("再生します");
	 }
	
	public void stop() {
		System.out.println("止まります");
	 }
	
	public void next() {
		System.out.println("次ぎにいきます");
	 }
	
	public void back() {
		System.out.println("戻ります");
	 }
	
	
}
